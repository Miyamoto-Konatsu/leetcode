class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
     map<int, int> s;
    vector<int> b(2);
    for (int i = 0; i < nums.size(); i++)
    {
        if (s.count(target - nums[i]) > 0)
        {
            b[0] = s[target - nums[i]];
            b[1] = i;
            break;
        }
        s[nums[i]] = i;
    }

    return b;
    }
};
